<#--package ${gen.modelPackage?replace("/",".")};-->

<#--import java.lang.reflect.Method;-->
<#--import org.springframework.web.bind.annotation.RequestMapping;-->
<#--import ${gen.annotationPackage?replace("/",".")}.*;-->


<#--/**-->
 <#--* 方法包装-->
 <#--*/-->
<#--public class MethodWrapper {-->

    <#--//映射路径，包含Class路径+Method路径-->
    <#--private String requestMappingPath;-->

    <#--private AMenu menu;-->
    <#--private Authority authority;-->

    <#--private RequestMapping requestMapping;-->
    <#--private Method method;-->
    <#---->
    <#--public String getRequestMappingPath() {-->
        <#--return requestMappingPath;-->
    <#--}-->

    <#--public MethodWrapper setRequestMappingPath(String requestMappingPath) {-->
        <#--this.requestMappingPath = requestMappingPath;-->
        <#--return this;-->
    <#--}-->

    <#--public Method getMethod() {-->
        <#--return method;-->
    <#--}-->

    <#--public MethodWrapper setMethod(Method method) {-->
        <#--this.method = method;-->
        <#--return this;-->
    <#--}-->

    <#--public Authority getAuthority() {-->
        <#--return authority;-->
    <#--}-->

    <#--public MethodWrapper setAuthority(Authority authority) {-->
        <#--this.authority = authority;-->
        <#--return this;-->
    <#--}-->

    <#--public Menu getMenu() {-->
        <#--return menu;-->
    <#--}-->

    <#--public MethodWrapper setMenu(Menu menu) {-->
        <#--this.menu = menu;-->
        <#--return this;-->
    <#--}-->

    <#--public RequestMapping getRequestMapping() {-->
        <#--return requestMapping;-->
    <#--}-->

    <#--public MethodWrapper setRequestMapping(RequestMapping requestMapping) {-->
        <#--this.requestMapping = requestMapping;-->
        <#--return this;-->
    <#--}-->
<#--}-->